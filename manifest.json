{
  "manifest_version": 3,
  "name": "Mobbin Cracker",
  "version": "1.0",
  "description": "自动将Mobbin上小尺寸图片(w<80)替换为高清图片(w=1920)",
  "action": {
    "default_icon": {

    }
  },
  "permissions": [
    "activeTab",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.mobbin.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {

  }
}